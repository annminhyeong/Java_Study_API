public class Project05_A_Client {
	
	public static void main(String[] args) {
		try {
			//소켓 만들기
			Socket socket = new Socket("127.0.0.1",9999); // -----> accept()
			System.out.println("Connect Success");
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
